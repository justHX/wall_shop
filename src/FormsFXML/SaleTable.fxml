<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderSale" prefHeight="517.0" prefWidth="754.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wall_shop.SaleTableController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="buttAddSal" mnemonicParsing="false" text="Добавить запись" />
            <Button fx:id="buttSaveSal" mnemonicParsing="false" text="Сохранить запись" />
            <Button fx:id="buttRemSal" mnemonicParsing="false" text="Удалить запись" />
        </buttons>
      </ButtonBar>
   </bottom>
   <center>
      <TableView fx:id="tableSell" editable="true" prefHeight="534.0" prefWidth="754.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="idSell" prefWidth="75.0" text="ID" visible="false" />
          <TableColumn fx:id="dateSell" prefWidth="100.0" text="Дата" />
            <TableColumn fx:id="resSell" prefWidth="100.0" text="№ Резерва" />
            <TableColumn fx:id="moneySell" prefWidth="110.0" text="Цена" />
        </columns>
      </TableView>
   </center>
</BorderPane>
