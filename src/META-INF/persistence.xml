<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Wall_ShopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>wall_shop.entyties.Country</class>
    <class>wall_shop.entyties.InfoGarages</class>
    <class>wall_shop.entyties.Garages</class>
    <class>wall_shop.entyties.Reservet</class>
    <class>wall_shop.entyties.Wallpapers</class>
    <class>wall_shop.entyties.Companys</class>
    <class>wall_shop.entyties.Clients</class>
    <class>wall_shop.entyties.Provider</class>
    <class>wall_shop.entyties.Color</class>
    <class>wall_shop.entyties.Delivery</class>
    <class>wall_shop.entyties.Selling</class>
    <class>wall_shop.entyties.Party</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/wall_shop?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
